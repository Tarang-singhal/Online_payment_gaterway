<%- include('partials/header.ejs')-%>
    <div class="row justify-content-md-center gap">
        <div class="col-md-6">
            <form action="/home/login" method="POST">
                <h2>Login</h2>
                <div class="form-group">
                    <label for="login_id">Usename</label>
                    <input type="text" name="username" class="form-control" id="login_id" placeholder="username">
                    <small class="form-text text-muted">We'll never share your email with anyone else.</small>
                </div>
                <div class="form-group">
                    <label for="password_id">Password</label>
                    <input type="password" name="password" class="form-control" id="password_id" placeholder="password">
                </div>
                <button type="submit" class="btn btn-primary">Login</button>
            </form>
        </div>
    </div>
    <%- include('partials/footer.ejs')-%>